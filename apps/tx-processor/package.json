    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
