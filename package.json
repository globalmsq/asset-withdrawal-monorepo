{
  "name": "asset-withdrawal-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn install",
    "build": "nx run-many --target=build --exclude=tx-monitor",
    "clean": "nx run-many --target=clean && rm -rf node_modules .nx/cache",
    "dev": "nx run-many --target=dev --parallel",
    "lint": "nx run-many --target=lint",
    "lint:fix": "nx run-many --target=lint --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "test": "nx run-many --target=test",
    "typecheck": "nx run-many --target=typecheck",
    "coverage": "nx run-many --target=test --coverage",
    "depcheck": "nx run-many --target=depcheck",
    "prepare": "husky install",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "docker:up": "docker-compose -f docker/docker-compose.yaml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yaml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yaml logs -f",
    "docker:clean": "docker-compose -f docker/docker-compose.yaml down -v",
    "docker:rebuild": "docker-compose -f docker/docker-compose.yaml build --no-cache",
    "dev:local": "npm run docker:up && npm run dev",
    "dev:logs": "npm run docker:logs"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@nx/esbuild": "^21.3.11",
    "@nx/eslint": "21.2.1",
    "@nx/eslint-plugin": "21.2.1",
    "@nx/jest": "21.2.1",
    "@nx/js": "21.2.2",
    "@nx/node": "21.2.2",
    "@nx/workspace": "21.2.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vercel/ncc": "^0.38.3",
    "depcheck": "^1.4.7",
    "esbuild": "^0.19.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^28.2.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.4.1",
    "lint-staged": "^15.2.2",
    "nx": "21.2.1",
    "prettier": "^3.2.5",
    "prisma": "^6.11.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.840.0",
    "@prisma/client": "^6.11.0",
    "@types/jsonwebtoken": "^9.0.10",
    "ethers": "^6.13.4",
    "express": "^4.21.2",
    "express-rate-limit": "^6.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "tslib": "^2.3.0",
    "winston": "^3.0.0",
    "zod": "^3.0.0"
  }
}
